<h1>NEW STAMP</h1>

<!-- pass local variable @response for this to work -->
<%= form_for @demo do |d| %>
	<fieldset>
		<%= d.label :stamp_serial, "Stamp Serial" %>
		<%= d.text_field :stamp_serial, :value => @form_filler %>

		<%#= image_tag @demo.stamp_image.url(:medium) %>
		<%= d.file_field :stamp_image %><br />
	</fieldset>

	<%= d.submit %>
<% end %>